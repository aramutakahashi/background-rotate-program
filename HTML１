<script src="js/jquery-3.2.1.min.js"></script>
<script type="text/javascript">  
$(function(){
var SampleVal =10,
    SampleNum = 0,
    SampleNow =0;
jQuery.fn.DoSample = function(config){
return this.each(function(i,elem){
           SampleNum++;
           SampleNow++;
           $('#body').css("background-posion","100px 100px"{,'transform':'rotate('+SampleVal*SampleNum+'deg)'});
           }
           });
           }
$('#body'). animate(function(){
SampleTimer = setInterval(function(){$('#body').DoSample()},infinite);
});
});
</script>
